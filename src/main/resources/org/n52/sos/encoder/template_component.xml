<component name="@procedure-id@">
	<Component>
		<identification>
			<IdentifierList>
				<identifier name="URN">
					<Term definition="urn:ogc:def:identifier:OGC:1.0:uniqueID">
						<value>@procedure-resource@</value>
					</Term>
				</identifier>
				<identifier name="shortName">
					<Term definition="urn:ogc:def:identifier:OGC:1.0:shortName">
						<value>@procedure-id@</value>
					</Term>
				</identifier>
			</IdentifierList>
		</identification>
		<capabilities>
			<swe:DataRecord>
				<swe:field name="cadence">
					<!-- Bezieht sich die Kadenzzeit auf die aggregationTypes? Dann wÃ¼rde 
						es Sinn machen, die hier mit reinzuschreiben! -->
					<swe:Quantity definition="urn:x-ogc:def:property:equidistance">
						<swe:uom code="h" />
						<swe:value>1</swe:value>
					</swe:Quantity>
				</swe:field>
				@procedure-features@
			</swe:DataRecord>
		</capabilities>
		<inputs>
			<InputList>
				<input name="ambient-air">
					<swe:ObservableProperty definition="ambient-air" />
				</input>
			</InputList>
		</inputs>
		
		<outputs>
			<OutputList>
			    @procedure-outputs@
			</OutputList>
		</outputs>
	</Component>
</component>