<sos:observationData>
	<om:OM_Observation gml:id="@observation-id@">
		<om:type xlink:href="http://www.opengis.net/def/observationType/OGC-OM/2.0/OM_Measurement"/>
		<om:phenomenonTime>
			<gml:TimeInstant gml:id="phenomenonTime">
				<gml:timePosition>@observation-phentime@</gml:timePosition>
			</gml:TimeInstant>
		</om:phenomenonTime>
		<om:resultTime xlink:href="#phenomenonTime"/>
		<om:procedure xlink:href="@observation-procedure@"/>
		<om:observedProperty xlink:href="@observation-property@"/>
		<om:featureOfInterest xlink:href="@observation-feature@"/>
		<om:result xsi:type="gml:MeasureType" uom="@observation-ucum@">@observation-result@</om:result>
	</om:OM_Observation>
</sos:observationData>