<swes:description>
	<swes:SensorDescription>
		<swes:data>
			<PhysicalSystem gml:id="@procedure-id@">
				<gml:description>@procedure-desc@</gml:description>
			    <gml:identifier codeSpace="">@procedure-id@</gml:identifier>
			    <gml:boundedBy>
					<gml:Envelope>
						<gml:lowerCorner>@procedure-location-lowerCorner@</gml:lowerCorner>
						<gml:upperCorner>@procedure-location-upperCorner@</gml:upperCorner>
					</gml:Envelope>
				</gml:boundedBy>
				<identification>
					  <IdentifierList>
						 <identifier>
							<Term definition="urn:ogc:def:identifier:OGC:shortname">
							   <label>short name</label>
							   <value>@procedure-id@</value>
							</Term>
						 </identifier>
						 <identifier>
							<Term definition="urn:ogc:def:identifier:OGC:longname">
							   <label>long name</label>
							   <value>@procedure-name@</value>
							</Term>
						 </identifier>
					  </IdentifierList>
				</identification>
				<classification>
					<ClassifierList>
						<classifier>
							<Term definition="urn:ogc:def:classifier:OGC:1.0:application">
								<label>intendedApplication</label>
								<value>@procedure-app@</value>
							</Term>
						</classifier>
						<classifier>
							<Term definition="http://www.opengis.net/def/property/OGC/0/SensorType">
							   <label>sensorType</label>
							   <value>@procedure-type@</value>
							</Term>
						 </classifier>
					</ClassifierList>
				</classification>
				
				<contacts>
					<ContactList>
						<member>
							<gmd:CI_ResponsibleParty>
								<gmd:individualName>
									<gco:CharacterString>@contact-name@</gco:CharacterString>
								</gmd:individualName>
								<gmd:positionName>
									<gco:CharacterString>@contact-posistion@</gco:CharacterString>
								</gmd:positionName>
								<gmd:contactInfo>
									<gmd:CI_Contact>
										<gmd:address>
											<gmd:CI_Address>
												<gmd:deliveryPoint>
													<gco:CharacterString>@contact-street@</gco:CharacterString>
												</gmd:deliveryPoint>
												<gmd:city>
													<gco:CharacterString>@contact-city@</gco:CharacterString>
												</gmd:city>
												<gmd:country>
													<gco:CharacterString>@contact-country@</gco:CharacterString>
												</gmd:country>
												<gmd:electronicMailAddress>
													<gco:CharacterString>@contact-email@</gco:CharacterString>
												</gmd:electronicMailAddress>
											</gmd:CI_Address>
										</gmd:address>
									</gmd:CI_Contact>
								</gmd:contactInfo>
								<gmd:role>
									<gmd:CI_RoleCode codeList="someServer/codeList.xml#CI_RoleCode" codeListValue="pointOfContact"/>
								</gmd:role>
							</gmd:CI_ResponsibleParty>
						</member>
					</ContactList>
				</contacts>
				
				<outputs>
					<OutputList>
						@procedure-outputs@
					</OutputList>
				</outputs>
				
			</PhysicalSystem>
		</swes:data>
	</swes:SensorDescription>
</swes:description>