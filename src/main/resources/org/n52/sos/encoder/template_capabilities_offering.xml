<swes:offering>
	<sos:ObservationOffering>
		<swes:identifier>@offering-identifier@</swes:identifier>
		<swes:procedure>@offering-procedure@</swes:procedure>
		<!-- currently not supported: <swes:procedureDescriptionFormat>http://www.opengis.net/sensorML/2.0</swes:procedureDescriptionFormat>-->
		<swes:procedureDescriptionFormat>http://www.opengis.net/sensorML/1.0.1</swes:procedureDescriptionFormat>
		@offering-observable-properties@
		@offering-observed-area@
		<sos:phenomenonTime>
			<gml:TimePeriod gml:id="phenomenonTime">
				<gml:beginPosition>@offering-begin-position@</gml:beginPosition>
				<gml:endPosition>@offering-end-position@</gml:endPosition>
			</gml:TimePeriod>
		</sos:phenomenonTime>
	</sos:ObservationOffering>
</swes:offering>